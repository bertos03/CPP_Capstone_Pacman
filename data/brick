    createAd: function() {
      window.nitroAds.queue.push(["createAd", arguments]);
    },
    queue: []
  };
  window["__DAADS__"] = {ev: []};
  function rec(ev, e) {
    if (window["__DAADS__"].ev === false) {
      return;
    }
    window["__DAADS__"].ev.push({ev: ev, e: e.detail, t: performance.now()});
  }
  document.addEventListener('nitroAds.loaded', (e) => { rec('loaded', e); });
